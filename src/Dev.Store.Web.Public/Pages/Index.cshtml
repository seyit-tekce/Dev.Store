@page
@using Dev.Store.Web.Public.Pages.IndexComponents.HomeSliderComponent;
@using Microsoft.AspNetCore.Mvc.Localization
@using Dev.Store.Localization
@using Volo.Abp.Users
@model Dev.Store.Web.Public.Pages.IndexModel
@inject IHtmlLocalizer<StoreResource> L
@inject ICurrentUser CurrentUser
@section styles {
    <abp-style src="/Pages/Index.css" />
}
@section scripts {
    <abp-script src="/Pages/Index.js" />
}
@{
    ViewBag.FluidLayout = true;
}


<!-- Home slider -->
@await Component.InvokeAsync(typeof(Dev.Store.Web.Public.Pages.IndexComponents.HomeSliderComponent.HomeSliderComponent),new HomeSliderModel())
<!-- Home slider end -->
<!-- collection banner -->
@await Component.InvokeAsync(typeof(Dev.Store.Web.Public.Pages.IndexComponents.CollectionBannerComponent.CollectionBannerComponent))
<!-- collection banner end -->
<!-- Tab product -->
@await Component.InvokeAsync(typeof(Dev.Store.Web.Public.Pages.IndexComponents.TabProductComponent.TabProductComponent),new Dev.Store.Web.Public.Pages.IndexComponents.TabProductComponent.TabProductComponentModel())
<!-- Tab product end -->
<!-- Parallax banner -->
@await Component.InvokeAsync(typeof(Dev.Store.Web.Public.Pages.IndexComponents.ParallaxBannerComponent.ParallaxBannerComponent))
<!-- Parallax banner end -->
<!-- Blog-->
@await Component.InvokeAsync(typeof(Dev.Store.Web.Public.Pages.IndexComponents.BlogComponent.BlogComponent))
<!-- Blog end -->
